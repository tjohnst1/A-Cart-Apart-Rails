<% if current_user %>
  <%= link_to 'Add a Food Cart', new_food_cart_path, remote: true, id: 'add-food-cart', class:"btn btn-default" %>
  <%= link_to 'Sign Out', destroy_user_session_path, method: 'delete', class:"btn btn-default" %>
<% else %>
    <%= link_to new_user_registration_path, class:"btn btn-default" do %>
      <span class="glyphicon glyphicon-user"></span> Sign Up
    <% end %>
    <%= link_to new_user_session_path, class:"btn btn-default" do %>
      <span class="glyphicon glyphicon-log-in"></span> Sign In
    <% end %>
<% end %>
