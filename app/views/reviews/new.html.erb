<div id="new-review-form-container">
  <%= form_for [@food_cart, @review], remote: true do |f| %>
    <div class="form-group">
      <%= f.label :rating %>
      <%= f.text_field :rating, class:"form-control" %>
    </div>
    <div class="form-group">
      <%= f.label :content %>
      <%= f.text_area :content, class:"form-control" %>
    </div>
    <%= f.submit 'Submit', class: "btn btn-primary" %>
    <a class="btn btn-default" data-dismiss="modal" href="#">Cancel</a>
  <% end %>
</div>
